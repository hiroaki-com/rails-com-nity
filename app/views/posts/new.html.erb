<div class="padding-both-side">
    <h2>投稿を作成する</h2>
    <%= form_with model: @post, local: true do |f| %>
        <%= f.text_area :content, rows: 5 %>
        <%= f.file_field :images, multiple: true %><br>
        <div class="submit-block">
            <%= f.submit class: "button" %>       
        </div>
    <% end %>

    <div class= "skip-top">
        <p><%= link_to 'Top', root_path %></p>
    </div>


</div>